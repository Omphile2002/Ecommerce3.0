<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.mobilemarket.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_upload_image"
        android:name="com.example.mobilemarket.UploadImage"
        android:label="@string/title_upload_image"
        tools:layout="@layout/activity_upload_image" />

    <fragment
        android:id="@+id/navigation_view"
        android:name="com.example.mobilemarket.ViewItems"
        android:label="@string/title_view"
        tools:layout="@layout/fragment_view_content" />

</navigation>